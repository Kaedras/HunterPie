<UserControl x:Name="MinimalBar" x:Class="HunterPie.GUIControls.Custom_Controls.MinimalHealthBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <UserControl.Resources>
    </UserControl.Resources>
    <Grid>
        <Rectangle x:Name="HealthBarBackground" Fill="{Binding ElementName=MinimalBar, Path=Background}"/>
        <Rectangle x:Name="HealthBar" HorizontalAlignment="Left" Margin="0" Fill="{Binding ElementName=MinimalBar, Path=Color}"/>
    </Grid>
</UserControl>
